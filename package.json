{
  "name": "enhancely-api-docs",
  "version": "1.0.0",
  "description": "Builds static API documentation using ReDoc (Community Edition)",
  "private": true,
  "scripts": {
    "build-docs": "redocly build-docs openapi/openapi_v1.yaml --output docs/index.html --theme.openapi.expandSingleSchemaField=true --theme.openapi.jsonSampleExpandLevel=all"
  },
